{
  "name": "spencer-github-actions-demo",
  "version": "0.0.1",
  "description": "Demo project for GitHub Actions CI and Changesets release",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean --sourcemap",
    "test": "vitest run --coverage",
    "dev:test": "vitest",
    "changeset": "changeset",
    "version:ci": "changeset version",
    "release": "changeset publish",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spencer/github-actions-demo.git"
  },
  "author": "Spencer",
  "license": "MIT",
  "packageManager": "npm@10",
  "dependencies": {},
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@types/node": "^20.14.11",
    "tsup": "^8.1.0",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4"
  }
}